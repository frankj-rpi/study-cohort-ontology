sco-i:MetforminArm
       a    owl:Class, sco:InterventionArm; 
       rdfs:subClassOf        sio:StudySubject;
       owl:EquivalentClassOf [ owl:IntersectionOf (sio:StudySubject [
           a owl:Restriction ;
           owl:onProperty sio:isParticipantIn ;
           owl:hasValue sco-i:10YearFollowUpStudy 
  	]
	[
           a owl:Restriction ;
           owl:onProperty sio:isParticipantIn ;
           owl:someValuesFrom sco:ConventionalTherapy 
  	]
	[
           a owl:Restriction ;
           owl:onProperty sio:isParticipantIn ;
           owl:someValuesFrom [owl:IntersectionOf (provcare:Intervention [
           a owl:Restriction ;
           owl:onProperty prov:used;
           owl:someValuesFrom chebi:6801 
  	])
			       ]
  	])];
       sio:hasAttribute    
       [ a sco:PopulationSize; sio:hasValue 309],
       [ a ncit:C25298; rdfs:label "Systolic Blood Pressure";
          sio:hasAttribute
          [ a sio:Mean; sio:hasValue 139],
          [a sio:StandardDeviation; sio:hasValue 22]  
        ] . 

sco-i:10YearFollowUpStudy
	a  owl:NamedIndividual , ncit:C71104 ;
	rdfs:seeAlso  "https://www.ncbi.nlm.nih.gov/pubmed/18784090" ;
	dct:creator     "Holman RR.", "Paul S.K.", "Bethel M.A.", "Matthews D.R.", "Neil H.A." ;
	dct:title       "10-year follow-up of intensive glucose control in type 2 diabetes" ;
	sm:documentURL  
	"https://www.nejm.org/doi/full/10.1056/NEJMoa0806470?url_ver=Z39.88-2003&rfr_id=ori%3Arid%3Acrossref.org&rfr_dat=cr_pub%3Dpubmed" ;
	sio:hasParticipant   sco-i:MetforminArm .
