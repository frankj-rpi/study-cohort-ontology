            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>dual therapy</rdfs:label>
    </owl:Class>




    <!-- &therapies;Monotherapy -->

    <owl:Class rdf:about="&therapies;Monotherapy">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sio;hasValue"/>
                <owl:hasValue rdf:datatype="&xsd;string">Mono Therapy</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>monotherapy</rdfs:label>
    </owl:Class>
    


    <!-- &therapies;HyperglycemiaRescue -->

    <owl:Class rdf:about="&therapies;HyperglycemiaRescue">
    </owl:Class>


