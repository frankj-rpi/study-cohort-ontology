  <owl:Class rdf:about="http://hadatac.org/ont/chear#Father">
	<rdfs:label>Father</rdfs:label>
	<hasco:wasApprovedBy>chear-kb:PER-Jeanette-Stingone</hasco:wasApprovedBy>
	<rdfs:subClassOf>
	  <owl:Restriction>
		<owl:allValuesFrom>
		  <owl:Restriction>
