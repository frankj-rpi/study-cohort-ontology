    <rdfs:subClassOf rdf:resource="http://hadatac.org/ont/chear#Relative"/>
    <skos:definition>An ancestor or forebear is a parent or (recursively) the parent of an ancestor (i.e., a grandparent, great-grandparent, great-great-grandparent, and so forth). Ancestor is &quot;any person from whom one is descended. In law the person from whom an estate has been inherited.&quot;</skos:definition>
  </owl:Class>

  <owl:Class rdf:about="http://hadatac.org/ont/chear#Descendant">
    <rdfs:label>Descendant</rdfs:label>
    <hasco:wasApprovedBy>chear-kb:PER-Jeanette-Stingone</hasco:wasApprovedBy>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:intersectionOf>
              <rdf:Description>
                <rdf:first>
                  <owl:Restriction>
                    <owl:allValuesFrom rdf:resource="http://hadatac.org/ont/chear#Ancestor"/>
