    <skos:notation>65</skos:notation>
  </hasco:QualityValue>

  <hasco:QualityValue rdf:about="http://purl.obolibrary.org/obo/NCIT_C75563">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Unemployment</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/ExO_0000034"/>
