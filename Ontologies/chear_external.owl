    <rdfs:subClassOf rdf:resource="http://hadatac.org/ont/chear#Relative"/>
    <skos:definition>The child of a human, or any grandchild, great-grandchild, and so on.</skos:definition>
  </owl:Class>

  <owl:Class rdf:about="http://hadatac.org/ont/chear#Child">
    <rdfs:label>Child</rdfs:label>
    <hasco:wasApprovedBy>chear-kb:PER-Jeanette-Stingone</hasco:wasApprovedBy>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
