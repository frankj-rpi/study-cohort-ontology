    <skos:notation>85</skos:notation>
  </hasco:QualityValue>

  <hasco:QualityValue rdf:about="http://purl.obolibrary.org/obo/NCIT_C75560">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Homemaker</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/ExO_0000034"/>
    <skos:definition>A person who manages a home as their main daily activity.</skos:definition>
    <skos:notation>68</skos:notation>
  </hasco:QualityValue>

  <hasco:QualityValue rdf:about="http://purl.obolibrary.org/obo/NCIT_C75561">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Student</rdfs:label>
