    <skos:definition>Indicates a person whose marriage has been legally dissolved and has not remarried.</skos:definition>
    <skos:notation>40</skos:notation>
  </hasco:QualityValue>

  <hasco:QualityValue rdf:about="http://purl.obolibrary.org/obo/NCIT_C51777">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Separated</rdfs:label>
    <rdfs:subClassOf>
      <hasco:QualityValue rdf:about="http://purl.obolibrary.org/obo/NCIT_C51773">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:label>Married</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://hadatac.org/ont/chear#MaritalStatus"/>
