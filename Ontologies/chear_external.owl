    <skos:notation>42</skos:notation>
  </hasco:QualityValue>

  <hasco:QualityValue rdf:about="http://purl.obolibrary.org/obo/NCIT_C67109">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Multiracial</rdfs:label>
    <hasco:wasApprovedBy>chear-kb:PER-Jeanette-Stingone</hasco:wasApprovedBy>
    <rdfs:subClassOf rdf:resource="http://semanticscience.org/resource/Race"/>
    <skos:definition>Having ancestors of several or various races.</skos:definition>
    <skos:editorialNote>Not part of current prov for Race. Should we remove this concept?</skos:editorialNote>
    <skos:notation>54</skos:notation>
  </hasco:QualityValue>

  <hasco:QualityValue rdf:about="http://purl.obolibrary.org/obo/NCIT_C67137">
