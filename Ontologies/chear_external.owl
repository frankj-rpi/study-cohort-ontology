  <owl:Class rdf:about="http://hadatac.org/ont/chear#Mother">
    <rdfs:label>Mother</rdfs:label>
    <hasco:wasApprovedBy>chear-kb:PER-Jeanette-Stingone</hasco:wasApprovedBy>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Restriction>
            <owl:onProperty rdf:resource="http://semanticscience.org/resource/hasAttribute"/>
            <owl:someValuesFrom rdf:resource="http://semanticscience.org/resource/Female"/>
          </owl:Restriction>
