        <skos:notation>37</skos:notation>
      </hasco:QualityValue>
    </rdfs:subClassOf>

    <skos:definition>Indicates a person living apart from his/her spouse by legal arrangement.</skos:definition>
    <skos:notation>41</skos:notation>
  </hasco:QualityValue>

  <hasco:QualityValue rdf:about="http://purl.obolibrary.org/obo/NCIT_C52658">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Full Time Employment</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/ExO_0000034"/>
    <skos:definition>Employed for a standard number of hours of working time, at least 50% or 20 hours per week.</skos:definition>
    <skos:notation>64</skos:notation>
  </hasco:QualityValue>

  <hasco:QualityValue rdf:about="http://purl.obolibrary.org/obo/NCIT_C53262">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
