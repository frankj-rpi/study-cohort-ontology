    <skos:notation>67</skos:notation>
  </hasco:QualityValue>

  <hasco:QualityValue rdf:about="http://purl.obolibrary.org/obo/NCIT_C76240">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Annulled</rdfs:label>
    <rdfs:subClassOf>
      <hasco:QualityValue rdf:about="http://purl.obolibrary.org/obo/NCIT_C51774">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:label>Never Married</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://hadatac.org/ont/chear#Unmarried"/>
