  <owl:Class rdf:about="http://hadatac.org/ont/chear#Ancestor">
    <rdfs:label>Ancestor</rdfs:label>
    <hasco:wasApprovedBy>chear-kb:PER-Jeanette-Stingone</hasco:wasApprovedBy>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:intersectionOf>
              <rdf:Description>
                <rdf:first>
                  <owl:Restriction>
                    <owl:allValuesFrom rdf:resource="http://hadatac.org/ont/chear#Descendant"/>
