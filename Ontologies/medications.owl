<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY dct "http://purl.org/dc/terms/" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY medications "https://purl.org/heals/medications/" >
    <!ENTITY chear "http://hadatac.org/ont/chear#" >
    <!ENTITY hasco "http://hadatac.org/ont/hasco#" >
    <!ENTITY fibo-fnd-utl-av "http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/">
    <!ENTITY sio "http://semanticscience.org/resource/">
    <!ENTITY obo "http://purl.obolibrary.org/obo/">
    <!ENTITY sco "http://purl.org/sco/">
    <!ENTITY medications "http://purl.org/medications/"> 
    <!ENTITY provcare "http://www.case.edu/ProvCaRe/provcare#">    
]>

<rdf:RDF xml:base="https://purl.org/heals/medications/"
     xmlns:chebi="http://purl.obolibrary.org/obo/CHEBI_"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:entity_attribute1="http://purl.obolibrary.org/obo/envo-p/patterns/entity_attribute/"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:hp="http://purl.obolibrary.org/obo/HP_"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:doid="http://purl.obolibrary.org/obo/doid#"
     xmlns:Wikipedia="http://purl.obolibrary.org/obo/Wikipedia#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:envo="http://purl.obolibrary.org/obo/envo#"
     xmlns:cito="http://purl.org/spar/cito/"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:core="http://purl.obolibrary.org/obo/core#"
     xmlns:doap="http://usefulinc.com/ns/doap#"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:sio="http://semanticscience.org/resource/"
     xmlns:uo="http://purl.obolibrary.org/obo/UO_"
     xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:uberon="http://purl.obolibrary.org/obo/uberon#"
     xmlns:oboInOwl="http://www.geneontology.org/formats/oboInOwl#"
     xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
     xmlns:hasco="http://hadatac.org/ont/hasco#"
     xmlns:ubprop="http://purl.obolibrary.org/obo/ubprop#"
     xmlns:entity_attribute="http://purl.obolibrary.org/obo/oba/patterns/entity_attribute/"
     xmlns:chear="http://hadatac.org/ont/chear#"
     xmlns:sco="http://purl.org/heals/sco/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:hsapdv="http://purl.obolibrary.org/obo/hsapdv#"
     xmlns:entity_quality="http://purl.obolibrary.org/obo/oba/patterns/entity_quality/"
     xmlns:obo="http://purl.obolibrary.org/obo/"
     xmlns:process_attribute="http://purl.obolibrary.org/obo/envo-p/patterns/process_attribute/"
    xmlns:provcare="http://www.case.edu/ProvCaRe/provcare#"
     >
    <owl:Ontology rdf:about="https://purl.org/heals/medications/">
        <owl:versionIRI rdf:resource="&medications;1.0.0"/>
        <owl:imports rdf:resource="https://purl.org/heals/sco/"/>     
    </owl:Ontology>


    <!-- &medications;Insulin -->

    <owl:Class rdf:about="&medications;Insulin">
        <rdfs:subClassOf rdf:resource="&provcare;ClinicalIntervention"/>
    </owl:Class> 


    <!-- &medications;OralAntidiabetesDrug -->

    <owl:Class rdf:about="&medications;OralAntidiabetesDrug">
        <rdfs:subClassOf rdf:resource="&provcare;ClinicalIntervention"/>
        <rdfs:label>oral antidiabetes drug</rdfs:label>
    </owl:Class>    



    <!-- &medications;SulfonyureaInsulin -->

    <owl:Class rdf:about="&medications;SulfonyureaInsulin">
        <rdfs:subClassOf rdf:resource="&provcare;ClinicalIntervention"/>
        <rdfs:label>sulfonylurea + insulin</rdfs:label>
    </owl:Class>

    <!-- &medications;Metformin -->

    <owl:Class rdf:about="&medications;Metformin">
        <rdfs:subClassOf rdf:resource="&provcare;ClinicalIntervention"/>
        <rdfs:label>metformin</rdfs:label>
    </owl:Class>    
    

</rdf:RDF>
